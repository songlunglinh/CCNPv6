----------------------------------------------------------CONFIG R1-----------------------------------------------------------
en
config t
hostname R1
enable secret darkness
line con 0
password hntu
login
line vty 0 4
password hntu
login
exit
int loopback 11
ip add 10.1.1.1 255.255.255.252
exit
int loopback 15
ip add 10.1.1.5 255.255.255.252
exit
int loopback 19
ip add 10.1.1.9 255.255.255.252
exit
int s0/0/0
description R1-->R2
clock rate 128000
bandwidth 128
ip add 10.1.102.1 255.255.255.248
no shutdown
exit
int s0/0/1
description R1-->R3
ip address 10.1.103.1 255.255.255.248
no shutdown
exit
router eigrp 100
network 10.0.0.0
end
copy r s

